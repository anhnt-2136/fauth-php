{
    "name": "anhnt-2136/fauth-php",
    "keywords": [
        "laravel",
        "oauth",
        "authentication"
    ],
    "description": "Authentication User Framgia",
    "type": "library",
    "license": "Framgia",
    "authors": [
        {
            "name": "Tran Dinh Vi",
            "email": "trandinhvi39@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "illuminate/contracts": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Framgia\\Fauth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Framgia\\Fauth\\FAuthServiceProvider"
            ],
            "aliases": {
                "Fauth": "Framgia\\Fauth\\Facades\\Fauth"
            }
        }
    }
}
